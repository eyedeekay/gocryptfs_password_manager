#! /usr/bin/env sh


if [ -z $GPM_name_field ]; then
    GPM_name_field="name"
fi

if [ -z $GPM_pass_field ]; then
    GPM_pass_field="pass"
fi

if [ -f /etc/gpm.conf ]; then
    . /etc/gpm.conf
fi

curl --cookie "$GPM_user_bin_cookies" --cookie-jar "$GPM_user_bin_cookies" \
    -d "$GPM_name_field=$NAME_user" \
    -d "$GPM_pass_field=$PASS_user" \
    $@
